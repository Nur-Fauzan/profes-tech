            "Customer": @Html.Raw(ordersList.Customer.ToClientList(ordersList)),
            "Address": @Html.Raw(ordersList.Address.ToClientList(ordersList)),
        })
        .build();
    window[form.id] = form;
    currentForm = form;
    loadjs.done(form.id);
});
</script>
